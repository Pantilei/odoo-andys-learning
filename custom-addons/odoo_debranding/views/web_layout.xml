<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="web_layout" inherit_id="web.layout">
        <xpath expr="//link[@type='image/x-icon']" position="replace">
            <link type="image/x-icon" rel="shortcut icon" href="/web/binary/company_logo" />
        </xpath>
        <xpath expr="//title" position="replace">
            <title t-esc="'S.R.L. TRABO-PLUS'" />
        </xpath>
    </template>

    <template id="web_brand_promotion_message" inherit_id="web.brand_promotion_message">
        <xpath expr="//t[@t-out]" position="replace">
            <t t-out="''" />
        </xpath>
    </template>

